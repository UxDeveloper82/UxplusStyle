@model UxDesignPlusStyle.Models.Movie
@{
    ViewBag.Title = "Model.Name";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
<h2>@Model.Name</h2>
    <ul>
        <li>@Model.Genre.Name</li>
        <li>Release Date: @Model.ReleaseDate.ToLongDateString()</li>
        <li>Date Added: @Model.DateAdded.ToLongTimeString()</li>
        <li>Number in Stock: @Model.NumberInStock</li>
    </ul>
</div>